{
  "name": "qadhya",
  "version": "0.1.0",
  "private": true,
  "description": "Qadhya - Plateforme SaaS de gestion de cabinet juridique pour avocats tunisiens",
  "author": "Qadhya Team",
  "license": "UNLICENSED",
  "scripts": {
    "seed": "node scripts/create-default-user.js",
    "predev": "npm run seed",
    "dev": "next dev -p 7002",
    "prebuild": "npm run seed",
    "build": "next build",
    "prestart": "npm run seed",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:rag": "vitest run __tests__/lib/ai/rag-chat-service.test.ts __tests__/lib/ai/kb-quality-analyzer-service.test.ts lib/ai/__tests__/kb-duplicate-detector.test.ts",
    "test:rag:watch": "vitest --watch __tests__/lib/ai/rag-chat-service.test.ts __tests__/lib/ai/kb-quality-analyzer-service.test.ts lib/ai/__tests__/kb-duplicate-detector.test.ts",
    "test:coverage:rag": "vitest --coverage __tests__/lib/ai/rag-chat-service.test.ts __tests__/lib/ai/kb-quality-analyzer-service.test.ts lib/ai/__tests__/kb-duplicate-detector.test.ts",
    "test:citations": "vitest run __tests__/lib/ai/citation-validator-service.test.ts",
    "test:scalability": "NODE_OPTIONS='--expose-gc' npx tsx scripts/test-indexing-scalability.ts",
    "test:classification-apis": "npx tsx scripts/test-classification-apis.ts",
    "audit:rag": "npx tsx scripts/audit-rag-data-quality.ts",
    "audit:rag:json": "npx tsx scripts/audit-rag-data-quality.ts --export=json",
    "audit:rag:csv": "npx tsx scripts/audit-rag-data-quality.ts --export=csv",
    "rechunk:large": "npx tsx scripts/rechunk-large-documents.ts",
    "rechunk:large:dry-run": "npx tsx scripts/rechunk-large-documents.ts --dry-run",
    "migrate:api-keys": "npx tsx scripts/migrate-platform-configs-to-api-keys.ts",
    "check:translations": "node scripts/check-translations.js",
    "i18n:check": "npx tsx scripts/i18n-check.ts",
    "find:hardcoded-text": "node scripts/find-hardcoded-text.js",
    "check:dark": "./scripts/check-dark-mode.sh",
    "fix:dark": "./scripts/fix-dark-mode.sh",
    "translations:validate": "npm run check:translations && npm run find:hardcoded-text",
    "precommit": "npm run check:translations",
    "stop": "lsof -ti:7002 | xargs kill -9 2>/dev/null || true",
    "tunnel:start": "./scripts/tunnel-prod-db.sh start",
    "tunnel:stop": "./scripts/tunnel-prod-db.sh stop",
    "tunnel:status": "./scripts/tunnel-prod-db.sh status",
    "check:env": "node scripts/check-env-sync.js",
    "init:minio": "npx tsx scripts/init-minio-buckets.ts",
    "check:minio": "npx tsx scripts/check-minio-health.ts",
    "sync:export": "bash scripts/sync-to-prod.sh",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules/.cache",
    "restart": "npm run stop && sleep 2 && npm run clean && sleep 1 && npm run dev",
    "restart:full": "npm run stop && sleep 2 && npm run clean:all && sleep 1 && npm run dev",
    "health": "curl -s http://localhost:7002/api/health | jq . || echo 'Serveur non accessible'",
    "monitor": "./scripts/dev-monitor.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:abrogation": "playwright test e2e/workflows/abrogation-detection.spec.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts || echo 'Integration tests not configured'",
    "test:e2e:rag": "playwright test e2e/workflows/rag-*.spec.ts || echo 'E2E RAG tests not configured'",
    "storage:cleanup": "node scripts/cleanup-storage-manual.js",
    "storage:analyze": "node scripts/analyze-storage.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@getbrevo/brevo": "^3.0.1",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",

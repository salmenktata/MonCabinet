    "remark-gfm": "^4.0.1",
    "resend": "^4.0.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "swr": "^2.4.0",
    "tailwind-merge": "^2.6.1",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^5.1.1",
    "umap-js": "^1.4.0",
    "use-debounce": "^10.1.0",
